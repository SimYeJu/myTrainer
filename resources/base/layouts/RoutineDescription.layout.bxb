layout-macro-def (routine-description){
  params {
      param (routinedescript) {
         type (RoutineRecommendResults)
         min (Required) max (One)
      }
   }

  content {
    section {
      content {
        paragraph {
          value ("#{value(routinedescript.rDesc)}")
          style (Title_L)
        }
      }
    }
  }
}
