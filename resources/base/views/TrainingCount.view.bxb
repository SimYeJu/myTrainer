result-view {
	match: TrainingCountResults (result) {
		from-output: TrainingCount
	}
	
	
	message {
		template ("#{value(result.tName)} 한 세트 카운트를 시작합니다."){
			speech ("#{value(result.speech)}")
		}
	}
	
	render {
			layout-macro (count-result) {
				param (countresult) {
					expression (result)
				}
			}
	}
	conversation-drivers {
		conversation-driver {
			template ("팔굽혀펴기 알려줘")
		}
	}
}