result-view {
   match: TrainingDescriptionResults (trainingresult) {
      from-output: TrainingDetail
   }

   message {
      template ("운동 루틴에 대한 정보입니다.")
   }
   
   render{ 
    if (size(trainingresult) == 1) {
			layout-macro (training-detail) {
        param (trainingresult) {
          expression (trainingresult)
        }
      }
		}
   }   
   conversation-drivers {
      conversation-driver {
         template ("하체운동 알려줘")
      }
   }
}